{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "google", "prettier"],

  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "quotes": ["error", "double"],
    "indent": "off",
    "react/forbid-prop-types": 0,
    "linebreak-style": 0,
    "object-curly-spacing": "off",
    "require-jsdoc": 0,
    "react/prop-types": 0,
    "operator-linebreak": [
      1,
      "after",
      {
        "overrides": {
          "?": "ignore",
          ":": "ignore"
        }
      }
    ],
    "comma-dangle": [0, "always-multiline"]
  }
}
